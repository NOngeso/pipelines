    "    except ValueError:\n",
    "        genelist = ['.'.join(x.split('.')[0:2]) for x in genelist]\n",
    "    cor = []\n",
    "    for i, geneleft in enumerate(genelist):\n",
    "        try:\n",
    "            generight = genelist[i+1]\n",
    "        except IndexError:\n",
    "            continue\n",
    "        try:\n",
    "            cor.append(df.loc[geneleft]['GeneRightCor'])\n",
    "        except KeyError:\n",
    "            continue\n",
    "    return cor\n",
